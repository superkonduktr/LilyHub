<% provide :title, "Browse" %>

<div class="container">
  <div class="row">
    <div class="col-lg-offset-1 col-lg-6 col-md-8 col-sm-12 col-xs-12">
      <h1>Latest engravings</h1>
      <%= render 'layouts/pagination' %>
      <% @engravings.each do |engraving| %>
        <%= render partial: "engraving", locals: { engraving: engraving } %>
      <% end %>
      <%= render 'layouts/pagination' %>
    </div>
  </div>
</div>
